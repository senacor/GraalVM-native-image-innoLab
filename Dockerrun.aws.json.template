{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "$NAME",
        "image": "$AWS_ACCOUNT_ID.dkr.ecr.eu-central-1.amazonaws.com/$NAME:$TAG",
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 8080
          }
        ],
        "essential": true,
        "memory": 128
      }
    ]
}
